{
  "name": "brreg",
  "description": "Look up data from the Norwegian Entity Registry",
  "version": "4.0.0",
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no"
  },
  "license": "MIT",
  "keywords": [
    "brreg",
    "difi",
    "enhetsregisteret",
    "norwegian entity registry",
    "Brønnøysundregistrene"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/brreg.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/brreg/issues"
  },
  "homepage": "https://github.com/zrrrzzt/brreg",
  "main": "index.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "test": "standard && npm audit && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "devDependencies": {
    "coveralls": "3.0.3",
    "standard": "12.0.1",
    "tap": "13.0.4"
  },
  "dependencies": {
    "difi": "5.0.2"
  }
}
