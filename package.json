{
  "name": "brreg",
  "description": "Look up data from the Norwegian Entity Registry",
  "version": "3.0.0",
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no"
  },
  "license": "MIT",
  "keywords": [
    "brreg",
    "difi",
    "enhetsregisteret",
