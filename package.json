  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/brreg.git"
  },
  "bugs": {
    "url": "https://github.com/zrrrzzt/brreg/issues"
  },
  "homepage": "https://github.com/zrrrzzt/brreg",
  "main": "index.js",
  "engines": {
    "node": ">=8.8.1"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
